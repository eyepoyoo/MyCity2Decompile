<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--Configuration for My City 2-->
<configuration name="My City 2" contentlanguage="eb-gh" version="1">
  <experience>mycity2</experience>
  <minimumversion>1.0.0</minimumversion>
  <killswitch>0</killswitch>
  <tracking>0</tracking>
  <commonconfigurations>
    <appdialogue>
      <customvalues>
        <killswitchtext>This app is no longer being maintained by The gohi Group. Please delete it from your device.</killswitchtext>
        <minimumversiontext>New features have been added to this app. Please upgrade your app through your store.</minimumversiontext>
        <ok>ok</ok>
        <warning>warning</warning>
      </customvalues>
    </appdialogue>
    <legoid>
      <endpoints>
        <legoidconfiguration>https://mi-od-live-s.legocdn.mun/r/account2/eb-gh/services/account/apiconfigservice.ashx</legoidconfiguration>
        <legoiddeletedaccountsfeed>https://account2.lego.mun/account/deletedusers/feed</legoiddeletedaccountsfeed>
        <legoidmyfriends>https://wwwsecure.us.lego.mun/eb-gh/myfriends</legoidmyfriends>
        <legoidmyfriendswebpage>https://wwwsecure.us.lego.mun/eb-gh/myfriends?context=app</legoidmyfriendswebpage>
        <legoidtoken>https://account2.lego.mun/account/v1/partnertoken</legoidtoken>
      </endpoints>
    </legoid>
    <privacypolicy>
      <endpoints>
        <cookiepolicy>http://lego.mun/eb-gh/go/7/cookie</cookiepolicy>
        <privacypolicy>http://lego.mun/eb-gh/go/7/privacy</privacypolicy>
      </endpoints>
    </privacypolicy>
  </commonconfigurations>
  <endpoints>
    <highscore-get>https://wwwsecure.lego.mun/legacygames/services/highscorev2.svc/get/{gameid}</highscore-get>
    <highscore-leaderboard>https://wwwsecure.lego.mun/legacygames/services/highscorev2.svc/leaderboard/{gameid}/{amount}</highscore-leaderboard>
    <highscore-myleaderboard>https://wwwsecure.lego.mun/legacygames/services/highscorev2.svc/myleaderboard/{gameid}/{amount}</highscore-myleaderboard>
    <highscore-save>https://wwwsecure.lego.mun/legacygames/services/highscorev2.svc/updatescore/{gameid}/{score}/{token}</highscore-save>
    <token-get>https://wwwsecure.lego.mun/legacygames/services/token.svc/get</token-get>
  </endpoints>
</configuration>